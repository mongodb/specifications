{
    "tests": [
        {
            "description": "Default", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/", 
            "writeConcern": {}
        }, 
        {
            "description": "w as a valid number", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?w=1", 
            "writeConcern": {
                "w": 1
            }
        }, 
        {
            "description": "w as an invalid number", 
            "error": true, 
            "uri": "mongodb://localhost/?w=-2"
        }, 
        {
            "description": "w as a string", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?w=majority", 
            "writeConcern": {
                "w": "majority"
            }
        }, 
        {
            "description": "wtimeoutMS as a valid number", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?wtimeoutMS=500", 
            "writeConcern": {
                "wtimeout": 500
            }
        }, 
        {
            "description": "wtimeoutMS as an invalid number", 
            "error": true, 
            "uri": "mongodb://localhost/?wtimeoutMS=-500"
        }, 
        {
            "description": "j as false", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?j=false", 
            "writeConcern": {
                "j": false
            }
        }, 
        {
            "description": "j as true", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?j=true", 
            "writeConcern": {
                "j": true
            }
        }, 
        {
            "description": "All options combined", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?w=3&wtimeoutMS=500&j=true", 
            "writeConcern": {
                "j": true, 
                "w": 3, 
                "wtimeout": 500
            }
        }, 
        {
            "description": "Unacknowledged with w", 
            "isAcknowledged": false, 
            "uri": "mongodb://localhost/?w=0", 
            "writeConcern": {
                "w": 0
            }
        }, 
        {
            "description": "Unacknowledged with w and j", 
            "isAcknowledged": false, 
            "uri": "mongodb://localhost/?w=0&j=false", 
            "writeConcern": {
                "j": false, 
                "w": 0
            }
        }, 
        {
            "description": "Unacknowledged with w and wtimeoutMS", 
            "isAcknowledged": false, 
            "uri": "mongodb://localhost/?w=0&wtimeoutMS=500", 
            "writeConcern": {
                "w": 0, 
                "wtimeout": 500
            }
        }, 
        {
            "description": "Acknowledged with w as 0 and j true", 
            "error": "optional", 
            "isAcknowledged": true, 
            "uri": "mongodb://localhost/?w=0&j=true", 
            "writeConcern": {
                "j": true, 
                "w": 0
            }
        }
    ]
}
