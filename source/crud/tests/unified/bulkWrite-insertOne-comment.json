{
  "description": "bulkWrite-insertOne-comment",
  "schemaVersion": "1.0",
  "runOnRequirements": [
    {
      "minServerVersion": "4.4"
    }
  ],
  "createEntities": [
    {
      "client": {
        "id": "client0",
        "observeEvents": [
          "commandStartedEvent"
        ]
      }
    },
    {
      "database": {
        "id": "database0",
        "client": "client0",
        "databaseName": "crud-v2"
      }
    },
    {
      "collection": {
        "id": "collection0",
        "database": "database0",
        "collectionName": "BulkWrite_insert_comment"
      }
    }
  ],
  "initialData": [
    {
      "collectionName": "BulkWrite_insert_comment",
      "databaseName": "crud-v2",
      "documents": []
    }
  ],
  "tests": [
    {
      "description": "BulkWrite insertOne with string comment",
      "operations": [
        {
          "object": "collection0",
          "name": "bulkWrite",
          "arguments": {
            "requests": [
              {
                "insertOne": {
                  "document": {
                    "_id": 1,
                    "x": "foo"
                  }
                }
              },
              {
                "insertOne": {
                  "document": {
                    "_id": 2,
                    "x": "bar"
                  }
                }
              }
            ],
            "comment": "comment"
          },
          "expectResult": {
            "deletedCount": 0,
            "insertedCount": 2,
            "matchedCount": 0,
            "modifiedCount": 0,
            "upsertedCount": 0,
            "upsertedIds": {}
          }
        }
      ],
      "expectEvents": [
        {
          "client": "client0",
          "events": [
            {
              "commandStartedEvent": {
                "command": {
                  "insert": "BulkWrite_insert_comment",
                  "documents": [
                    {
                      "_id": 1,
                      "x": "foo"
                    },
                    {
                      "_id": 2,
                      "x": "bar"
                    }
                  ],
                  "comment": "comment"
                }
              }
            }
          ]
        }
      ],
      "outcome": [
        {
          "collectionName": "BulkWrite_insert_comment",
          "databaseName": "crud-v2",
          "documents": [
            {
              "_id": 1,
              "x": "foo"
            },
            {
              "_id": 2,
              "x": "bar"
            }
          ]
        }
      ]
    },
    {
      "description": "BulkWrite insertOne with document comment",
      "operations": [
        {
          "object": "collection0",
          "name": "bulkWrite",
          "arguments": {
            "requests": [
              {
                "insertOne": {
                  "document": {
                    "_id": 1,
                    "x": "foo"
                  }
                }
              },
              {
                "insertOne": {
                  "document": {
                    "_id": 2,
                    "x": "bar"
                  }
                }
              }
            ],
            "comment": {
              "key": "value"
            }
          },
          "expectResult": {
            "deletedCount": 0,
            "insertedCount": 2,
            "matchedCount": 0,
            "modifiedCount": 0,
            "upsertedCount": 0,
            "upsertedIds": {}
          }
        }
      ],
      "expectEvents": [
        {
          "client": "client0",
          "events": [
            {
              "commandStartedEvent": {
                "command": {
                  "insert": "BulkWrite_insert_comment",
                  "documents": [
                    {
                      "_id": 1,
                      "x": "foo"
                    },
                    {
                      "_id": 2,
                      "x": "bar"
                    }
                  ],
                  "comment": {
                    "key": "value"
                  }
                }
              }
            }
          ]
        }
      ],
      "outcome": [
        {
          "collectionName": "BulkWrite_insert_comment",
          "databaseName": "crud-v2",
          "documents": [
            {
              "_id": 1,
              "x": "foo"
            },
            {
              "_id": 2,
              "x": "bar"
            }
          ]
        }
      ]
    }
  ]
}
